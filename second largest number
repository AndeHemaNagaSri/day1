{
    int a,b,c;
    printf("enter the number 1:");
    scanf("%d",&a);
 printf("enter the number 2:");
    scanf("%d",&b);
     printf("enter the number 3:");
    scanf("%d",&c);
    if(a>b && a>c)
    {
        if(b>c);
            printf("%d is second largest num",b);
        else
            printf("%d is the second largest num",c);
    }
    else if(b>a && b>c)
